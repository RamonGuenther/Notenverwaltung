    <artifactId>Notenverwaltung</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>Notenverwaltung</name>
    <description>Eine Applikation zum verwalten von Noten f√ºr den Studiengang Informatik an der FHSWF</description>
    <properties>
        <java.version>17</java.version>
        <javafx.version>15.0.1</javafx.version>
    </properties>
    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter</artifactId>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>

        <dependency>
            <groupId>net.rgielen</groupId>
